<div class="container">
    <p class="title text-center">experiencia laboral 👩‍🚀 </p>
    {{#each experience}}
        <div class="grid-2 text-center bg-cinco">
            <div class="title-dos">
                <p>{{ puesto }}</p>
            </div>
            <div class="title-dos">
                <p>{{ empresaTiempo }}</p>
            </div>
        </div>
        <div class="grid-0 pb-3 bg-dos">
            <div class="mt-1 sub-title-dos p-3 ">
                {{#each skills}}
                    <p>{{ descripcion }}</p>
                {{/each}}
                <hr class="mt-1">
            </div>
        </div>
    {{/each}}
</div>
